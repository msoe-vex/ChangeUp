                    "severity": 4,
                    "message": 5
                }
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "v5_hal Firmware Upload",
            "type": "shell",
            "command": "prosv5.exe upload",
            "options": {
                "cwd": "${workspaceFolder}/src/v5_hal/firmware"
            },
            "problemMatcher": {
                "owner": "prosv5",
                "fileLocation": [
                    "relative",
                    "${workspaceRoot}"
                ],
                "pattern": {
                    "regexp": "^(.*):(\\d+):(\\d+): +(warning|error): +(.*)",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            },
            "group": "build"
